    \u043B\u0443\u0447\u0448\u0435 \u0432\u0441\u0435\u0433\u043E, \u0435\u0441\u043B\u0438
    \u0432\u044B \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0435\u0441\u044C \u043D\u0430
    \u043C\u044F\u0433\u043A\u043E\u0439, \u043D\u043E \u0442\u0432\u0435\u0440\u0434\u043E\u0439
    \u043F\u043E\u0432\u0435\u0440\u0445\u043D\u043E\u0441\u0442\u0438, \u043D\u0430\u043F\u0440\u0438\u043C\u0435\u0440,
    \u043D\u0430 \u043A\u043E\u0432\u0440\u0435 \u0438\u043B\u0438 \u043D\u0430 \u043A\u043E\u0432\u0440\u0438\u043A\u0435
    \u0434\u043B\u044F \u0443\u043F\u0440\u0430\u0436\u043D\u0435\u043D\u0438\u0439
    \u043D\u0430 \u043F\u043E\u043B\u0443.\r\n\r\n\u0428\u0430\u0433 2 \u0421\u043E\u0433\u043D\u0438\u0442\u0435
    \u043A\u043E\u043B\u0435\u043D\u0438.\r\n\r\n\u0421\u043E\u0433\u043D\u0438\u0442\u0435
    \u043A\u043E\u043B\u0435\u043D\u0438. \u041B\u0435\u0436\u0430 \u043D\u0430 \u0441\u043F\u0438\u043D\u0435,
